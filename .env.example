# DeepAgents Environment Configuration Example

# API Keys (required for various agents)
TAVILY_API_KEY=your_tavily_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith Configuration (optional, for tracing)
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=your_project_name_here
LANGCHAIN_TRACING_V2=true

# LangChain v1 Core Feature Flag (optional, defaults to false)
# Set to 'true' to use LangChain v1 with SummarizationMiddleware
# Set to 'false' or omit to use current v0 implementation
USE_V1_CORE=false

# Model Configuration (optional)
# DEFAULT_MODEL=anthropic/claude-3-sonnet-20240229